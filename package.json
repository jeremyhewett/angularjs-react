{
  "name": "angularjs-react",
  "version": "1.0.0",
  "description": "A library for using React components as AngularJS directives",
  "main": "dist/angularjs-react.js",
  "scripts": {
    "build": "webpack --bail",
    "build:demo": "webpack --config=./demo/webpack.config.js --bail",
    "start": "webpack serve --config ./demo/webpack.config.js",
    "test": "protractor",
    "install:webdriver": "webdriver-manager update",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremyhewett/angularjs-react.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "react",
    "directive",
    "component",
    "transclude"
  ],
  "author": "Jeremy Hewett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeremyhewett/angularjs-react/issues"
  },
  "homepage": "https://github.com/jeremyhewett/angularjs-react#readme",
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "angular": "^1.8.3",
    "antd": "4.21.6",
    "babel-loader": "8.2.5",
    "css-loader": "^6.7.1",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "8.4.14",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "7.7.2",
    "protractor": "^5.1.2",
    "react": "^17",
    "react-dom": "^17",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-dom": ">=15",
    "angular": "1.x"
  }
